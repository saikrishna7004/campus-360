{
  "expo": {
    "name": "Campus 360",
    "slug": "campus-360",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-white.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.saikrishna7004.campus360"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./fonts/metropolis/Metropolis-Regular.otf",
            "./fonts/metropolis/Metropolis-Bold.otf",
            "./fonts/metropolis/Metropolis-ExtraBold.otf",
            "./fonts/metropolis/Metropolis-SemiBold.otf",
            "./fonts/metropolis/Metropolis-Medium.otf",
            "./fonts/metropolis/Metropolis-Light.otf",
            "./fonts/metropolis/Metropolis-Thin.otf",
            "./fonts/metropolis/Metropolis-Black.otf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "068e33e8-3b49-49ac-baa8-636eb1d21721"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/068e33e8-3b49-49ac-baa8-636eb1d21721"
    }
  }
}
